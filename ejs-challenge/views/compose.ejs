<%- include("partials/header") -%>

<h1>Compose</h1>

<form action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input class="form-control" name="titleInput" rows="1" cols="150"></textarea>
  </div>
  <div class="form-group">
    <label>Post</label>
    <textarea class="form-control" name="postInput" rows="5" cols="150"></textarea>
  </div>

  <button class="btn btn-primary" type="submit" name="submit">Publish</button>
</form>

<%- include("partials/footer") -%>
